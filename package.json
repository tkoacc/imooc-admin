    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "7.0.1",
    "lint-staged": "^11.2.6",
    "sass": "^1.77.0",
    "sass-loader": "^12.6.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
